-- Simple script for grinding strength and hiding frames
local player = game:GetService("Players").LocalPlayer
local replicatedStorage = game:GetService("ReplicatedStorage")

-- Hide frames
print("Attempting to hide frames...")
for _, obj in pairs(replicatedStorage:GetChildren()) do
    if obj.Name:match("Frame$") then
        pcall(function()
            obj.Visible = false
        end)
    end
end
print("Frame hiding complete")

-- Fast grinding function
print("Starting strength grinding...")

-- Create multiple threads for grinding
for i = 1, 10 do
    spawn(function()
        while true do
            pcall(function()
                player.muscleEvent:FireServer("rep")
            end)
            wait()
        end
    end)
end

print("Grinding started successfully!")
